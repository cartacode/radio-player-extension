{
  "name": "One Sream Player",
  "description": "Chrome Extension for One stream player",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
