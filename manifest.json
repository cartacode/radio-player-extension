{
  "name": "One Sream Player",
  "description": "Chrome Extension for One stream player",
  "version": "0.3",
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_popup": "tabs_api.html"
  },
  "manifest_version": 2
}
